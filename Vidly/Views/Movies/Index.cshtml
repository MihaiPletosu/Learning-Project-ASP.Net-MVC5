@model Vidly.ViewModels.MoviesViewModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table class="table table-bordered table-hover">
    <thead class="thead-dark">
        <tr>
            <th>Movie</th>
        </tr>
    </thead>
    @foreach (var movie in Model.Movies)
    {
        <tbody>
            <tr>
                <td>@movie.Name</td>
            </tr>
        </tbody>
    }
</table>

